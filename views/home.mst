{{>header}}

<div class="tags">
    {{#tags}}
        <div class="tag {{class}}" ><a href="/?t={{_id }}">{{_id}}({{count}})</a></div>
    {{/tags}}
</div>
TAG:{{tag}}
{{#posts}}
    <div class="post">
        {{#photo}}
        <img src="/media/{{photo}}" />
        {{/photo}}
    <h3><a href="/post/{{slug}}">{{title}}</a></h3>
    <small>Author: {{author}}</small>
    <P>{{body}}</p>
    {{#user}}
    <a href="/post/{{slug}}/edit">Editar</a>
    {{/user}}
    </div>  
    

{{/posts}}


