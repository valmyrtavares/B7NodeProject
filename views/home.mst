{{>header}}

<div class="tags">
    {{#tags}}
        <div class="tag">{{_id}}({{count}})</div>
    {{/tags}}
</div>
{{#posts}}
    <div class="post">
        {{#photo}}
        <img src="/media/{{photo}}" />
        {{/photo}}
    <h3><a href="/post/{{slug}}">{{title}}</a></h3>
    <P>{{body}}</p>
    <a href="/post/{{slug}}/edit">Editar</a>
    </div>  
    

{{/posts}}


